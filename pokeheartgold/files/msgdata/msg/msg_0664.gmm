	<row id="msg_0664_00015" index="15">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Hello?\rOh, hi, {STRVAR_1 3, 0, 0} darling!\nI love you and I miss you!\rCome home so we can sleep together *w*\r</language>
