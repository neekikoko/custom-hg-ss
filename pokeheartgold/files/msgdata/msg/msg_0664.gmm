	<row id="msg_0664_00003" index="3">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Hello?\rOh, hi, {STRVAR_1 3, 0, 0} darling! Chu!\nWhere are you, chu?\r{STRVAR_1 3, 0, 0}, I’m warming your bed for you, please visit soon, ok?</language>
	</row>
	<row id="msg_0664_00004" index="4">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Hello?\rOh, hi, {STRVAR_1 3, 0, 0}!\nWasn’t Professor Elm waiting for you?</language>
